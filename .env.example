# Environment Variables
NODE_ENV=development
PORT=3000

# Google Sheets API
GOOGLE_SHEET_ID=your_google_sheet_id_here
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./credentials/service-account-key.json

